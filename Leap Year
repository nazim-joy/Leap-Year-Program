#PROGRAM_LEAP_YEAR
PRINT*, "ENTER THE YEAR:"
READ*, N
IF (MOD(N,400)==0)THEN
  IF (MOD(N,100)==0)THEN
  PRINT*, "THIS IS A LEAP YEAR"
  ELSE
  PRINT*, "THIS IS NOT A LEAP YEAR"
  ENDIF
ELSE
  IF (MOD(N,4)==0)THEN
  PRINT*, "THIS IS A LEAP YEAR"
  ELSE
  PRINT*, "THIS IS NOT A LEAP YEAR"
  ENDIF
ENDIF
END PROGRAM
  
